/* pages/searchAll/searchAll.wxss */
@import "../public/iconfont";
@import "../public/public";
@import "../reserve/reserve";
page{
  background-color:#f5f5f5;
}
.search-head{
  position: fixed;
  top:0;
  left:0;
  width:calc(100% - 40rpx);
  height: 60rpx;
  padding:10rpx 20rpx;
  border-bottom:1px solid #eeeeee;
  background-color:#f5f5f5;
}
.search-head .l{
  width:85%;
  float:left;
  height:100%;
  border-radius: 10rpx;
  background-color:white;
}
.search-head .l .iconfont{
  width:50rpx;
  height: 100%;
  line-height: 60rpx;
  font-size:30rpx;
  text-align: center;
  float:left;
}
.search-head .l input{
  width:calc(100% - 50rpx);
  font-size:30rpx;
  height: 100%;
}
.search-head .r{
  width:15%;
  float:right;
  line-height: 60rpx;
  height: 100%;
  text-align: center;
  font-size:30rpx;
}
.search-head-height{height:80rpx; background-color:#f5f5f5;}
.isnull{height: calc(100vh - 80rpx); overflow: hidden; background-color:white;}
.isnull image{width:90%; display: block; margin:0 auto;}

.reserve-list {
 padding-bottom:0;
}

/* 顶部 */
.service-nav{
  position: fixed;
  top:80rpx;
  z-index: 99999;
  background-color: white;
  width:100%;
  left:0;
  height: 100rpx;
  border-bottom:1px solid #eeeeee;
}
.service-nav .li{
  width:50%;
  float:left;
  height:100rpx;
  line-height: 100rpx;
  text-align: center;
}
.service-nav .li.on>view{
position: relative;
}
.service-nav .li.on>view::before{
  content:'';
  position: absolute;
  bottom:0;
  left:40%;
  transform: translateX(-50%);
  width:40rpx;
  height:5rpx;
  background-color:#3d4bf6;
}
.service-nav .li.on text{
  color: #3d4bf6;
}
.service-nav .li.check>view{
position: relative;
}
.service-nav .li.check>view::before{
  content:'';
  position: absolute;
  bottom:0;
  left:40%;
  transform: translateX(-50%);
  width:40rpx;
  height:5rpx;
  background-color:#3d4bf6;
}
.service-nav .li.check text{
  color: #3d4bf6;
}
.service-nav .li view{
  display: inline-block;
}
.service-nav .li view text{
  font-size:30rpx;
  float:left;
  display: block;
  height:100rpx;
  line-height: 100rpx;
}
.service-nav .li view text.icon-xia1,.service-nav .li view text.icon-shang{
  font-size:25rpx;
  margin-left:10rpx;
  display: block;
  height:100rpx;
  line-height: 100rpx;
  float:right;
}
.service-nav-height{
  height: 100rpx;
}
.reserve-list .ul .li .text {
  margin-right:0;
}