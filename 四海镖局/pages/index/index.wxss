/**index.wxss**/
@import "../public/iconfont";
@import "../header/header";
@import "../footerNav/footerNav";
@import "../service/service";
/* 轮播 */
.banner{
  height: 400rpx;
}
.banner swiper-item image{
  width:100%;
}
/* 列表 */
.list{
  margin:25rpx 0;
  box-shadow: 0 10rpx 10rpx rgba(0,0,0,0.1);
}
.list-head{
 border-top:3rpx solid #dddddd;
 border-bottom: 3rpx solid #dddddd;
 line-height: 80rpx;
 height:80rpx;
 padding:0 10rpx;
}
.list-head-title{
  font-size:30rpx;
  color:#888888;
  display: block;
  float:left;
}
.list-head-open{
  font-size:30rpx;
  float: right;
  color:#888888;
  display: block;
}
.list-content{
  background-color: #f5f5f5;
  padding:20rpx 0;
}
.list-content .ul{
  overflow: hidden;
}
.list-content .ul .li{
  width:29.33%;
  float:left;
  margin:2%;
}
.list-content .ul .li view{
  margin:0 auto;
  width:100rpx;
  height:100rpx;
  overflow: hidden;
}
.list-content .ul .li view image{
  width:100%;
  height:100%;
}
.list-content .ul .li text{
  display: block;
  text-align: center;
  margin-top:15rpx;
  font-size:25rpx;
}
.list-content .ul .li.more view image{
  width:50%;
  height:50%;
  margin:25%;
}
