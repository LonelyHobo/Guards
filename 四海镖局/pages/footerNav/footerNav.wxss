/* pages/footerNav/footerNav.wxss */

.footerNav {
  position: fixed;
  z-index: 9999;
  bottom: 0;
  left:0;
  width:100%;
  height:80rpx;
  background-color:#5677fc;
}
.footer-height{
  height:80rpx;
}
.footerNav .ul{}
.footerNav .ul .li.lis{
  width:18.75%; 
  float:left; 
  color:white;
  font-size:28rpx;
  line-height: 80rpx;
  text-align: center;
}
.footerNav .ul .li.seek{
  width:25%;
  float:left; 
}
.footerNav .ul .li.seek>view{
  background-color:#e51c23;
  width:130rpx;
  height: 130rpx;
  display: block;
  position: relative;
  margin:0 auto;
  border-radius: 50%;
  transform: translateY(-50%);
}
.footerNav .ul .li.seek>view text{
  display: block;
  position: absolute;
  width:100%;
  left:0;
  top:50%;
  transform: translateY(-50%);
  line-height: 45rpx;
  height: 90rpx;
  color: white;
  font-size:35rpx;
  text-align: center;
}